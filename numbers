def convertStringToInt(a):
	print ("string a =", a)
	b = int(a)
	return b

def convertStringToFloat(a):
	print ("string a =", a)
	b = float(a)
	return b

def convertStringToComplex(a):
	print ("string a =", a)
	b = complex(a)
	return b

def floatPow(a,b):
	print ( a, " ^ ", b)
	return pow(a,b) 

def returnAbsolute(a):
	print ("The absolute value of ", a)
	return abs(a)

def returnMaxValueInList(a):
	print ("The max value in ", a)
	return max(a)

def returnMinValueInList(a):
	print ("The min value in ", a)
	return min(a)

def returnRoundedOfNumbers(a,b):
	print (a ,"Rounded unto ", b ," places")
	return round(a,b)

def returnHexadecimal(a):
	print ("The hexadecimal of a ", a)
	return hex(a)

def returnOctal(a):
	print ("The Octal of a", a)
	return oct(a)


print (convertStringToInt("5"))
print (" ")
print (convertStringToFloat("5"))
print (" ")
print (convertStringToComplex("5"))
print (" ")
print (floatPow(2,3))
print (" ")
print (returnAbsolute(-5.7))
print (" ")
print (returnMaxValueInList([1,5,4,7,3]))
print (" ")
print (returnMinValueInList([1,5,4,7,3]))
print (" ")
print (returnRoundedOfNumbers(4.239, 2))
print (" ")
print (returnHexadecimal(12))
print (" ")
print (returnOctal(12))


